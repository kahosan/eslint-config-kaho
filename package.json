{
  "name": "eslint-config-kaho",
  "version": "2.0.4",
  "description": "ESlint config for Kaho",
  "author": "kahosan <kahosan@outlook.com>",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kahosan/eslint-config-kaho.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "kahosan"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.cjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/kahosan/eslint-config-kaho/issues"
  },
  "homepage": "https://github.com/kahosan/eslint-config-kaho#readme",
  "scripts": {
    "build": "tsup index.ts --format esm,cjs --clean --dts",
    "lint": "eslint --format=sukka .",
    "release": "bumpp && pnpm run build && pnpm publish"
  },
  "dependencies": {
    "@antfu/eslint-config": "1.0.0-beta.18",
    "@eslint-sukka/js": "^4.0.1",
    "@eslint-sukka/legacy": "^4.0.1",
    "@eslint-sukka/node": "^4.0.1",
    "@eslint-sukka/react": "^4.0.1",
    "@eslint-sukka/shared": "^4.0.1",
    "@eslint-sukka/ts": "^4.0.1",
    "eslint-formatter-sukka": "^4.0.1"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "eslint-config-sukka": "^4.0.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "pnpm": {
    "overrides": {
      "array-includes": "npm:@nolyfill/array-includes@latest",
      "array.prototype.findlastindex": "npm:@nolyfill/array.prototype.findlastindex@latest",
      "array.prototype.flat": "npm:@nolyfill/array.prototype.flat@latest",
      "array.prototype.flatmap": "npm:@nolyfill/array.prototype.flatmap@latest",
      "array.prototype.tosorted": "npm:@nolyfill/array.prototype.tosorted@latest",
      "es-iterator-helpers": "npm:@nolyfill/es-iterator-helpers@latest",
      "has": "npm:@nolyfill/has@latest",
      "object.assign": "npm:@nolyfill/object.assign@latest",
      "object.entries": "npm:@nolyfill/object.entries@latest",
      "object.fromentries": "npm:@nolyfill/object.fromentries@latest",
      "object.groupby": "npm:@nolyfill/object.groupby@latest",
      "object.hasown": "npm:@nolyfill/object.hasown@latest",
      "object.values": "npm:@nolyfill/object.values@latest",
      "string.prototype.matchall": "npm:@nolyfill/string.prototype.matchall@latest"
    }
  }
}
