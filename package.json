{
  "name": "eslint-config-kaho",
  "version": "2.1.0-beta.5",
  "description": "ESlint config for Kaho",
  "homepage": "https://github.com/kahosan/eslint-config-kaho#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kahosan/eslint-config-kaho.git"
  },
  "bugs": {
    "url": "https://github.com/kahosan/eslint-config-kaho/issues"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.cjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup index.ts --format esm,cjs --clean --dts",
    "lint": "eslint .",
    "release": "bumpp && pnpm run build && pnpm publish"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "kahosan"
  ],
  "author": "kahosan <kahosan@outlook.com>",
  "license": "MIT",
  "dependencies": {
    "@eslint-sukka/shared": "^6.1.2",
    "eslint-config-sukka": "^6.1.2"
  },
  "devDependencies": {
    "eslint": "^9.6.0",
    "eslint-config-flat-gitignore": "^0.1.7",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3"
  },
  "pnpm": {
    "patchedDependencies": {
      "eslint-config-sukka@6.1.2": "patches/eslint-config-sukka@6.1.2.patch"
    }
  }
}
