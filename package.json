{
  "name": "eslint-config-kaho",
  "version": "2.0.8",
  "description": "ESlint config for Kaho",
  "author": "kahosan <kahosan@outlook.com>",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kahosan/eslint-config-kaho.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "kahosan"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.cjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/kahosan/eslint-config-kaho/issues"
  },
  "homepage": "https://github.com/kahosan/eslint-config-kaho#readme",
  "scripts": {
    "build": "tsup index.ts --format esm,cjs --clean --dts",
    "lint": "eslint --format=sukka .",
    "release": "bumpp && pnpm run build && pnpm publish"
  },
  "dependencies": {
    "@antfu/eslint-config": "1.0.0-beta.27",
    "@eslint-sukka/js": "^4.1.6",
    "@eslint-sukka/legacy": "^4.1.6",
    "@eslint-sukka/node": "^4.1.6",
    "@eslint-sukka/react": "^4.1.6",
    "@eslint-sukka/shared": "^4.1.6",
    "@eslint-sukka/ts": "^4.1.6",
    "eslint-formatter-sukka": "^4.1.6"
  },
  "devDependencies": {
    "eslint": "^8.51.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
