{
  "name": "eslint-config-kaho",
  "version": "2.0.15",
  "description": "ESlint config for Kaho",
  "author": "kahosan <kahosan@outlook.com>",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kahosan/eslint-config-kaho.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "kahosan"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.cjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/kahosan/eslint-config-kaho/issues"
  },
  "homepage": "https://github.com/kahosan/eslint-config-kaho#readme",
  "scripts": {
    "build": "tsup index.ts --format esm,cjs --clean --dts",
    "lint": "eslint .",
    "release": "bumpp && pnpm run build && pnpm publish"
  },
  "dependencies": {
    "@eslint-sukka/js": "^6.0.0-beta.7",
    "@eslint-sukka/legacy": "^6.0.0-beta.7",
    "@eslint-sukka/node": "^6.0.0-beta.7",
    "@eslint-sukka/react": "^6.0.0-beta.7",
    "@eslint-sukka/shared": "^6.0.0-beta.7",
    "@eslint-sukka/ts": "^6.0.0-beta.7"
  },
  "devDependencies": {
    "eslint": "^9.2.0",
    "eslint-config-flat-gitignore": "^0.1.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "typescript": ">= 5.3.3 < 5.4.0"
  }
}
